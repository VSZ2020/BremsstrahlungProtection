<Window x:Class="BSP.Source.XAML_Forms.InterpolationViewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BSP.Source.XAML_Forms"
        xmlns:dvg="clr-namespace:System.Windows.Forms.DataVisualization.Charting;assembly=System.Windows.Forms.DataVisualization"
        mc:Ignorable="d"
        Title="InterpolationViewer" Height="450" Width="800">
    <Grid>
        <WindowsFormsHost x:Name="chartContainer">
            <dvg:Chart x:Name="chart"/>
        </WindowsFormsHost>
    </Grid>
</Window>
